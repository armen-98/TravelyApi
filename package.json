{
  "name": "projects",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node bin/www.js",
    "watch": "nodemon bin/www.js",
    "db:model:generate": "npx sequelize-cli model:generate --name User --attributes firstName:STRING,lastName:STRING,email:STRING",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo:last:migration": "npx sequelize-cli db:migrate:undo",
    "db:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:create:seed": "npx sequelize-cli seed:generate --name demo-user",
    "db:all:seeds": "npx sequelize-cli db:seed:all",
    "db:run:seed": "npx sequelize-cli db:seed --seed name-of-seeder",
    "db:migrate:generate": "npx sequelize-cli migration:generate --name add-column-to-table"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "5.0.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemailer": "^6.10.1",
    "nodemon": "^3.1.10",
    "pg": "^8.15.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2"
  }
}
