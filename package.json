{
  "name": "projects",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "node bin/www.js",
    "build": "npm i && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "watch": "nodemon bin/www.js",
    "db:model:generate": "npx sequelize-cli model:generate --name TimeSlot --attributes title:STRING",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo:last:migration": "npx sequelize-cli db:migrate:undo",
    "db:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:undo:one": "npx sequelize-cli db:migrate:undo --name 20250428175311-create-setting.js",
    "db:create:seed": "npx sequelize-cli seed:generate --name demo-category-images",
    "db:seed:file": "npx sequelize-cli db:seed --seed 20250511133327-demo-users.js",
    "db:all:seeds": "npx sequelize-cli db:seed:all",
    "db:run:seed": "npx sequelize-cli db:seed --seed 20250517080228-demo-admin.js",
    "db:undo:seed:all": "npx sequelize-cli db:seed:undo:all",
    "db:undo:seed:one": "npx sequelize-cli db:seed:undo --seed 20250511133327-demo-users.js",
    "db:migrate:generate": "npx sequelize-cli migration:generate --name add-column-to-table"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.803.0",
    "@aws-sdk/s3-request-presigner": "^3.803.0",
    "@faker-js/faker": "^9.8.0",
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cron": "^4.3.0",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "5.0.0",
    "http-errors": "~1.6.3",
    "i18n": "^0.15.1",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.1",
    "nodemon": "^3.1.10",
    "pg": "^8.15.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "slugify": "^1.6.6",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2"
  }
}
